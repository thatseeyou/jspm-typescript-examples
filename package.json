{
  "name": "jspm-typescript-samples",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "build": "tsc -p docs",
    "build:watch": "npm run build && tsc -p docs -w",
    "build:trace": "tsc -p docs --traceResolution",
    "build:debug": "tsc -p docs --listFiles --listEmittedFiles",
    "serve": "lite-server",
    "bundle:parallax": "( cd docs && jspm bundle ./src-parallax/app/main ./src-parallax/build.js )",
    "bundle:jquery-parallax.js": "( cd docs && jspm bundle ./src-jquery-parallax.js/app/main ./src-jquery-parallax.js/build.js )",
    "bundle": "npm run bundle:parallax && npm run bundle:jquery-parallax.js",
    "unbundle": "jspm unbundle"
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "description": "",
  "devDependencies": {
    "jspm": "^0.16.52",
    "lite-server": "^2.2.2",
    "typescript": "^2.2.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "docs",
      "packages": "jspm_packages"
    },
    "configFile": "docs/systemjs.config.js",
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.32",
      "jquery": "npm:jquery@^2.2.4",
      "jquery-parallax.js": "npm:jquery-parallax.js@^1.4.3",
      "parallax": "github:wagerfield/parallax@^2.1.3"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "@types/jquery": "^2.0.40"
  }
}
